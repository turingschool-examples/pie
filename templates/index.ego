<%! func Index(w io.Writer, tables []*Table) error %>

<html>
<% head(w) %>

<body>
	<h1>PIE</h1>

	<h2>Tables</h2>

	<ul>
		<% for _, t := range tables { %>
			<li><a href="/tables/<%= t.Name %>"><%= t.Name %></a></li>
		<% } %>
	</ul>


	<br/><br/>
	<form class="dropzone" action="/tables"></form>

</body>
</html>

